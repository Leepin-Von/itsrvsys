<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plotech.kanban.mapper.FloPaperRouteMapper">
    <select id="nonAllowPaper" statementType="CALLABLE" resultType="com.plotech.kanban.pojo.dto.FloPaperRouteDTO">
        {CALL ERP_WEB_NonConfirm}
    </select>
    <select id="getEmpId" resultType="java.lang.String">
        SELECT EmpId FROM EmpBas(NOLOCK)
        WHERE EmpName = #{empName}
    </select>
</mapper>
